
<h1>Watch List</h1>


<% @lists.each do |list| %>
<h2><%= link_to list.name, list_path(list)%></h2>
<% end %>

<button type="button" class="btn btn-danger" data-toggle="button" aria-pressed="false">Delete List</button>
  <%= link_to "", list_path(@list), method: :delete, data: { confirm: "Delete list?" } %>
